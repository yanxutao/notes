
# wifi hotspot conf, located in /etc/NetworkManager/system-connections/

[connection]
id=tmp
uuid=aa7d79d6-d8c7-46cf-8ecc-8ea4b6e30f2e
type=wifi
permissions=
secondaries=

[wifi]
mac-address-blacklist=
mac-address-randomization=0
mode=ap
seen-bssids=
ssid=tmp

[wifi-security]
group=
key-mgmt=wpa-psk
pairwise=
proto=
psk=123456

[ipv4]
dns-search=
method=shared

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
ip6-privacy=0
method=ignore

